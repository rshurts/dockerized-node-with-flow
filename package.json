{
  "name": "dockerize-node",
  "version": "0.1.0",
  "description": "example of express app dockerized",
  "repository": "github:rshurts/dockerized-node",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "prebuild": "rm -rf dist",
    "build": "babel src -d dist --ignore *.test.js",
    "dev": "nodemon $NODE_DEBUG_OPTION --exec babel-node -- src/server.js",
    "lint": "eslint 'src/**/*.js'",
    "pretty": "eslint --fix 'src/**/*.js'",
    "flow": "flow check",
    "jest": "jest",
    "test": "yarn lint && yarn flow && yarn jest"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "Russell Shurts",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "flow-bin": "^0.52.0",
    "jest": "^20.0.4",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "supertest": "^3.0.0"
  }
}
